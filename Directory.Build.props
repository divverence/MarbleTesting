<Project>
  <PropertyGroup>
    <TargetFramework>net452</TargetFramework>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <PlatformTarget>x64</PlatformTarget>
    <DebugType>embedded</DebugType>
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>

  <PropertyGroup>
    <Copyright>Copyright 2015-2018 Divverence B.V.</Copyright>
    <Authors>Divverence B.V.</Authors>
    <PackageIconUrl>http://www.divverence.com/favicon.ico</PackageIconUrl>
  </PropertyGroup>

  <!-- PACKAGE AND VERSION CREATION -->
  <ItemGroup>
    <EmbeddedFiles Include="@(Compile);@(AdditionalFiles)" />
    <Compile Include="..\..\built\SharedAssemblyInfo*.cs" Link="SharedAssemblyInfo.cs" />
  </ItemGroup>

</Project>